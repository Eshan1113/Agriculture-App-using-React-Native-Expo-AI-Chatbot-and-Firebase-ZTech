{
  "expo": {
    "name": "Smart Farm",
    "slug": "EcoAgri",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eshan1999.EcoAgri"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a00f5e32-a02a-49a6-877c-6a6d0684fa68"
      }
    }
  }
}
